{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "cross-env PORT=5191 start-server-and-test dev http://localhost:{{PORT}} test:e2e",
    "test:e2e": "cypress run --config-file cypress.config.cjs",
    "test:e2e:open": "cypress open --config-file cypress.config.cjs",
    "build:highlight": "java -jar ../netlify/functions/antlr-compiler/antlr-4.13.1-complete.jar -Dlanguage=JavaScript -o src/grammar/generated -visitor -lib src/grammar/grammars src/grammar/grammars/ANTLRv4Lexer.g4 src/grammar/grammars/ANTLRv4Parser.g4"
  },
  "dependencies": {
    "@codemirror/commands": "^6.8.1",
    "@codemirror/language": "^6.11.1",
    "@codemirror/state": "^6.5.2",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@codemirror/view": "^6.37.2",
    "@lezer/highlight": "^1.2.1",
    "@s977121/start-server-and-test": "2.0.5-development",
    "@stagewise-plugins/react": "^0.4.9",
    "@stagewise/toolbar-react": "^0.4.9",
    "@types/codemirror": "^5.60.16",
    "@types/d3": "^7.4.3",
    "@uiw/codemirror-theme-dracula": "^4.23.13",
    "@uiw/codemirror-theme-github": "^4.23.13",
    "@uiw/codemirror-themes": "^4.23.13",
    "antlr4": "^4.13.2",
    "antlr4ng": "^3.0.16",
    "antlr4ts": "^0.5.0-alpha.4",
    "codemirror": "^6.0.2",
    "cross-env": "^7.0.3",
    "d3": "^7.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-resizable-panels": "^3.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "cypress": "^14.5.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
